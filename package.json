{
  "name": "smart-office-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "generate:rooms": "tsx scripts/generate-conference-rooms.ts",
    "generate:users": "tsx scripts/generate-users.ts",
    "generate:images": "tsx scripts/generate-room-images.ts",
    "generate:tickets": "tsx scripts/generate-service-tickets.ts",
    "generate:all": "npm run generate:rooms && npm run generate:users && npm run generate:images && npm run generate:tickets",
    "functions:serve": "supabase functions serve sensor-data-simulator",
    "functions:serve-booking": "supabase functions serve room-booking-simulator",
    "functions:serve-all": "supabase functions serve sensor-data-simulator && supabase functions serve room-booking-simulator",
    "simulate:bookings": "./scripts/simulate-bookings.sh",
    "simulate:sensors": "./scripts/simulate-sensors.sh",
    "demo:start-sensors": "curl -X POST \"http://localhost:54321/functions/v1/sensor-data-simulator\" -H \"Authorization: Bearer $(supabase status | grep 'anon key' | awk '{print $3}')\" -H \"Content-Type: application/json\" -d '{}'"
  },
